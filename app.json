{
  "expo": {
    "name": "gomango",
    "slug": "go-mango",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/go-mango-logo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/images/bootsplash_logo.png",
      "backgroundColor": "#061D37",
      "resizeMode": "cover"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.rinwaoluwa.gomango"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#061D37"
      },
      "package": "com.rinwaoluwa.gomango"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/IBMPlexSans-Medium.ttf",
            "./assets/fonts/IBMPlexSans-Regular.ttf",
            "./assets/fonts/IBMPlexSans-SemiBold.ttf",
            "./assets/fonts/PlusJakartaSans-Bold.ttf",
            "./assets/fonts/PlusJakartaSans-Medium.ttf",
            "./assets/fonts/PlusJakartaSans-Regular.ttf"
          ]
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "c9d29cc0-5c35-44e1-ac91-f818d7963b45"
      }
    }
  }
}
